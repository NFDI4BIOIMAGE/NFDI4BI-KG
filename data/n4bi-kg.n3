<https://doi.org/10.5281/zenodo.14381522>
        dc:contributor  [ foaf:firstName  "Lena";
                          foaf:lastName   "Krämer"
                        ];
        dc:contributor  [ foaf:firstName  "Tom";
                          foaf:lastName   "Boissonnet"
                        ];
        dc:creator      [ foaf:firstName  "Roman";
                          foaf:lastName   "Baum"
                        ];
        dc:creator      [ foaf:firstName  "Markus M.";
                          foaf:lastName   "Becker"
                        ];
        dc:creator      [ foaf:firstName  "Ihda";
                          foaf:lastName   "Chaerony Siffa"
