        dc:relation     <https://doi.org/10.5281/zenodo.8340247> , <https://zenodo.org/communities/cordi-2023/> , <https://zenodo.org/communities/nfdi4bioimage/>;
        dc:rights       "Creative Commons Attribution 4.0 International" , "https://creativecommons.org/licenses/by/4.0/legalcode" , "info:eu-repo/semantics/openAccess";
        dc:subject      "format" , "data" , "community" , "cloud" , "FAIR" , "bioimaging";
